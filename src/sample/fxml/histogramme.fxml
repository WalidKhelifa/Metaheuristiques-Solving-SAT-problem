<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.histogrammeController">
   <children>
      <BarChart fx:id="graphe" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #8764;" title="Génétique">
        <xAxis>
          <CategoryAxis label="Nombre d'execusion" side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" label="Taux de satisfiabilités" side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
